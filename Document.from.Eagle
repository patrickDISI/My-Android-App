fun main() {
    println(removeVowels(" What Are you doing"))
}
fun removeVowels(s: String): String {
    val vowel = StringBuilder()
    for (c in s) {
if (c != 'A' && c != 'a'
    && c != 'E' && c != 'e'
    && c != 'I' && c != 'i'
    && c != 'O' && c != 'o'
    && c != 'U' && c != 'u') {
    vowel.append(c)
   }
}
    return vowel .toString()
}